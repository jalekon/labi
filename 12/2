#include <iostream> 
#include <cmath> 
using namespace std;

int Sign(double x) {
	if (x < 0) {
		x = -1;
	}
	else if (x == 0) {
		x = 0;
	}
	else if (x > 0) {
		x = 1;
	};
	return x;
}

int main()
{
	setlocale(LC_ALL, "Russian");
	int y;
	double a, b;
	cout << "Введите 2 числа "<<endl;
	cin >> a >> b;
	y = Sign(a) + Sign(b);
	cout << y;
	}
