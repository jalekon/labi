#include <iostream>

#include <cmath>

using namespace std;

int main()
{
int x1, y1, x2, y2;
float s;
cout << "vvedite koordinati pervoi to4ki "<< endl;
cin >> x1 >> y1;
cout << "vvedite koordinati vtoroi to4ki "<< endl;
cin >> x2 >> y2;
s = sqrt(pow((x2 - x1), 2) + pow((y2 - y1), 2));
cout << "s= " << s;
}
