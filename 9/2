#include <iostream>

using namespace std;
int main()

{
	int n, c;
	cout << "viberite napravlenie" << endl << " sever = 0 " << endl <<  " vostok = 1 " << endl << " ug = 2 " << endl << " zapad = -1 " << endl<< "vvedite 4islo" << endl;
	cin >> c ;
	cout << "vvedite komandy";
	cin >> n;
	switch (c)
	{
	case 0:
		switch (n)
		{
		case 0:
			cout << "sever";
			break;
		case 1:
			cout << "vostok";
			break;
		case -1:
			cout << "zapad";
			break;
		}
		break;
	case 1:
		switch (n)
		{
		case 0:
			cout << "vostok";
			break;
		case 1:
			cout << "ug";
			break;
		case -1:
			cout << "sever";
			break;
		}
		break;
	case -1:
		switch (n)
		{
		case 0:
			cout << "zapad";
			break;
		case 1:
			cout << "sever";
			break;
		case -1:
			cout << "ug";
			break;
		}
	case 2:
		switch (n)
		{
		case 0:
			cout << "ug";
			break;
		case 1:
			cout << "zapad";
			break;
		case -1:
			cout << "vostok";
			break;
		}
	}
}
